{
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i src/assets/css/main.css -o _site/assets/css/main.css --watch --postcss",
